{
    "name": "flodaq/ticket-notification-bundle",
    "description": "This bundle provides e-mails notifications for Hackzilla Ticketing Symfony bundle every time a ticket is created or modified.",
    "keywords": [
        "ticket",
        "ticketing",
        "help desk",
        "helpdesk",
        "support",
        "multilingual",
        "notification"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Florent DAQUET",
            "email": "flodaq@linkthis.fr",
            "homepage": "http://linkthis.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "hackzilla/ticket-bundle": "3.1.x-dev",
        "symfony/config": "^2.8|^3.0|^4.0",
        "symfony/dependency-injection": "^2.8|^3.0|^4.0",
        "symfony/event-dispatcher": "^2.8|^3.0|^4.0",
        "symfony/http-kernel": "^2.8|^3.0|^4.0",
        "symfony/swiftmailer-bundle": "^2.3.9|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Flodaq\\TicketNotificationBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
