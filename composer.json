{
    "name": "flodaq/ticket-notification-bundle",
    "description": "This bundle provides e-mail notifications for HackzillaTicketBundle events.",
    "keywords": [
        "ticket",
        "ticketing",
        "help desk",
        "helpdesk",
        "support",
        "multilingual",
        "notification"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Florent DAQUET",
            "email": "flodaq@linkthis.fr",
            "homepage": "http://linkthis.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "hackzilla/ticket-bundle": "3.0.x-dev",
        "symfony/swiftmailer-bundle": "^2.3.9",
        "symfony/symfony": "^2.8 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "swiftmailer/swiftmailer": "^5.1",
        "symfony/phpunit-bridge": "^3.2 || ^4.0"
    },
    "conflict": {
        "phpunit/phpunit": "<5.4.3"
    },
    "autoload": {
        "psr-4": {
            "Flodaq\\TicketNotificationBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
